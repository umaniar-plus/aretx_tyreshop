<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="action_sale_order_jobcard" model="ir.actions.report">
        <field name="name">Print Job Card</field>
        <field name="model">sale.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">adv_features.report_saleorder_jobcard</field>
        <field name="binding_model_id" ref="sale.model_sale_order"/>
        <field name="binding_type">report</field>
    </record>

    <record id="action_claim_report_print" model="ir.actions.report">
        <field name="name">Print Claim Report</field>
        <field name="model">claim.report</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">adv_features.report_claim_report_template</field>
        <field name="binding_model_id" ref="model_claim_report"/>
        <field name="binding_type">report</field>
    </record>

    <record id="seq_claim_report" model="ir.sequence">
        <field name="name">Claim Report</field>
        <field name="code">claim.report</field>
        <field name="prefix">CRP</field>
        <field name="padding">4</field>
    </record>
</odoo>
