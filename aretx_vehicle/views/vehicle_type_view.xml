<?xml version="1.0" encoding="utf-8"?>
<odoo>
    #tree view
    <record id="type_tree_view" model="ir.ui.view">
        <field name="name">vehicle.type.tree</field>
        <field name="model">vehicle.type.model</field>
        <field name="arch" type="xml">
            <tree>
                <field name="vehicle_type_name"/>
            </tree>
        </field>
    </record>
    #form_view
    <record id="type_form_view" model="ir.ui.view">
        <field name="name">vehicle.type.form</field>
        <field name="model">vehicle.type.model</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="vehicle_type_name"/>
                    </group>
                </sheet>

            </form>
        </field>
    </record>
