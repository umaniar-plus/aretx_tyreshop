<?xml version="1.0" encoding="utf-8"?>
<odoo>
    #tree view
    <record id="brand_tree_view" model="ir.ui.view">
        <field name="name">vehicle.brand.tree</field>
        <field name="model">vehicle.brand.model</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
    #form_view
    <record id="brand_form_view" model="ir.ui.view">
        <field name="name">vehicle.brand.form</field>
        <field name="model">vehicle.brand.model</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>

            </form>
        </field>
    </record>
