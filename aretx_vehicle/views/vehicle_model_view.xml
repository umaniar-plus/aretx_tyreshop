<?xml version="1.0" encoding="utf-8"?>
<odoo>
     #tree view
    <record id="model_tree_view" model="ir.ui.view">
        <field name="name">vehicle.model.tree</field>
        <field name="model">vehicle.model.model</field>
        <field name="arch" type="xml">
           <tree>
                <field name="brand_id"/>
                <field name="name"/>
                <field name="type_id"/>

            </tree>
        </field>
    </record>
    #form_view
    <record id="model_form_view" model="ir.ui.view">
        <field name="name">vehicle.model.form</field>
        <field name="model">vehicle.model.model</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="brand_id"/>
                        <field name="name"/>
                        <field name="type_id"/>

                    </group>
                </sheet>

            </form>
        </field>
    </record>
